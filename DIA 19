#include<stdio.h>
#include<stdlib.h>

using namespace std;
//Añade una función que los ordene de mayor a menor
int main(){
	int numeros[10];
	int cont;
	int pos;
	int bolsillo;
	int nveces;
	printf("\nIntroduzca 10 numeros enteros:  ");
	for(cont=0;cont<10;cont++){
		printf("\nNumero &d:  ",cont);
		scanf("%d", &numeros[cont]);
		
	}
	for(nveces=0;nveces<10;nveces++){
		for(pos=0;pos<10;pos++){
			if(numeros[pos]>numeros[pos+1]){
			//intercambiamos 	
			bolsillo=numeros[pos];
			numeros[pos]=numeros[pos+1]	
			numeros[pos+1]=bolsillo;
			}
		}
	}
	printf("\n ORDENADA: ")
	for(conr=0;cont<10;cont++){
		printf("\nNumero %d: %d",cont,numeros[cont]);
	}
	return 0;
	
}
